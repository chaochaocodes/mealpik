<%= form_for @dish do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>

    <%= f.label :cuisine %><br>
    <%= f.text_field :cuisine %><br>

    <%= f.label :ease %><br>
    <%= f.text_field :ease %><br>

    <%= f.label :ingredient %><br>
    <%= f.text_field :ingredient %><br>

    <%= f.submit %>
<% end %>

